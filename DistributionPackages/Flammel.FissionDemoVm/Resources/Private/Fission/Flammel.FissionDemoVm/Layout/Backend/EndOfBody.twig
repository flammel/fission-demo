{% if fission().backend().active %}
    <div id="neos-new-backend-container"></div>
    <script
        data-neos-node="/sites/website@user-florianlammelgesagtgetanat;language=de"
        type="text/javascript"
    >
        window.onload = function() {
            try {
                sessionStorage.setItem(
                    "Neos.Neos.lastVisitedNode",
                    document
                        .querySelector("script[data-neos-node]")
                        .getAttribute("data-neos-node")
                );
            } catch (e) {}
        };
    </script>
    {{ fission().backend().unrenderedNodeInformation }}
    <script type="application/javascript">
        document.addEventListener("DOMContentLoaded", function() {
            var event = new CustomEvent("Neos.Neos.Ui.ContentReady");
            window.parent.document.dispatchEvent(event);
        });
    </script>
{% endif %}
